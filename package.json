{
  "name": "generator-erest",
  "version": "0.22.0",
  "description": "Yoman generator for erest. Easy build rest api",
  "files": [
    "utils",
    "erest",
    "generators"
  ],
  "scripts": {
    "format": "prettier --write \"./**/*.{js,ts,json,yaml}\"",
    "tag": "git tag v`node -p 'require(\"./package\").version'`",
