    "postpublish": "npm run tag",
    "test": "export NODE_ENV=test && mocha test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourtion/node-generator-erest.git"
  },
  "keywords": [
    "erest",
    "yeoman",
    "generator",
    "yeoman-generator"
  ],
  "author": "Yourtion <yourtion@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourtion/node-generator-erest/issues"
  },
  "homepage": "https://github.com/yourtion/node-generator-erest#readme",
  "dependencies": {
    "yeoman-generator": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-guo": "^0.8.0",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "yeoman-test": "^2.0.0"
  }
}
